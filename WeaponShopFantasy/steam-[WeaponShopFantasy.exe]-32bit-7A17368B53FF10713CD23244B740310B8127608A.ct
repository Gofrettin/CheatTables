label(mystuff)
label(cheat2_store1)
registersymbol(cheat2_store1)
label(cheat2_store2)
registersymbol(cheat2_store2)

cheat2_code:
  push -01 //skillID
  push edx //Receiver
  push ecx //Provider
  push eax //return-value
  pushfd
  cmp BYTE PTR [edx+69],01
  je mystuff
// 员工攻击敌方
default:
  popfd
  mov [cheat2_store2],edx
  mov [edx+2C],00
  jmp return
// 敌方攻击员工
mystuff:
  popfd
  mov [cheat2_store1],edx
  mov eax,00
  jmp return+05
cheat2_store1:
  DB 00 00 00 00
cheat2_store2:
  DB 00 00 00 00

cheat2_entry+454:
  jmp cheat2_code
return:

