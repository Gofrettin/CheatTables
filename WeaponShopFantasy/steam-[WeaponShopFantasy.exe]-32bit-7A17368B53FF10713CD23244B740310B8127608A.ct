// ecx 是新的金钱值
cheat1_code:
  mov ecx,[ebp-08]
  mov [eax+64],ecx
  push ecx
  mov ecx,eax
  add ecx,64
  mov [cheat1_store1],ecx
  pop ecx
  jmp return
// 金钱地址
cheat1_store1:
  DB 00 00 00 00

cheat1_entry+21:
